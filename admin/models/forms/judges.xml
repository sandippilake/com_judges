<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset name="country_details">
		<field name="country" type="text"
			class="inputbox"
			description="COM_COMPARE_COUNTRY_NAME"
			label="COM_COMPARE_COUNTRY_NAME"
			required="true"
			size="30"
		/>
		<field name="id" type="hidden"
			class="inputbox"
			description="COM_COMPARE_COMPANY_NAME"
			label="COM_COMPARE_COMPANY_NAME"
			
			
		/>
		
		
		<field
		name="state"
		type="list"
		class="chzn-color-state"
		default="1"
		size="1"
		label="JSTATUS"
		description="JFIELD_PUBLISHED_DESC">
		<option value="1">JPUBLISHED</option>
		<option value="0">JUNPUBLISHED</option>
		<option value="2">JARCHIVED</option>
		<option value="-2">JTRASHED</option>
	</field>
		<field
		name="high_pripority"
		type="list"
		class="inputbox"
		default="1"
		size="1"
		label="COM_COMPARE_HIGH_PRIPORITY"
		description="COM_COMPARE_HIGH_PRIPORITY">
		<option value="1">COM_COMPARE_YES</option>
		<option value="0">COM_COMPARE_NO</option>
		
	</field>
		<field
		name="default"
		type="list"
		class="inputbox"
		default="1"
		size="1"
		label="COM_COMPARE_DEFAULT"
		description="COM_COMPARE_DEFAULT">
		<option value="1">COM_COMPARE_YES</option>
		<option value="0">COM_COMPARE_NO</option>
		
	</field>

	

		
		
		
		
		
		
		


		

		
	</fieldset>
	

</form>
